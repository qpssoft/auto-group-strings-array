{
  "name": "auto-group-strings-array",
  "version": "1.1.1",
  "description": "Small JS library to group array of strings by common substring",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --declaration && npm run build:browser",
    "build:browser": "browserify -s autoGroupStrings src/index.ts -p [ tsify ] > dist/auto-group-strings.js && terser --compress --mangle -o dist/auto-group-strings.min.js dist/auto-group-strings.js",
    "test": "jest --config jestconfig.json",
    "coverage": "jest --config jestconfig.json --coverage"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/qpssoft/auto-group-strings-array.git"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "browserify": "^16.5.1",
    "jest": "^26.0.1",
    "terser": "^4.8.0",
    "ts-jest": "^26.1.1",
    "tsify": "^4.0.2",
    "typescript": "^3.9.5"
  },
  "keywords": [
    "string manipulation",
    "group strings"
  ],
  "author": "Quang Pham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qpssoft/auto-group-strings-array/issues"
  },
  "homepage": "https://github.com/qpssoft/auto-group-strings-array#readme",
  "dependencies": {
    "auto-group-strings": "^1.1.0"
  }
}
